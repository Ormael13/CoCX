    <logic>
        <!--

        Syntax:

        Type item = show | hide | if | switch

        Element <show part=""/>
            attribute [part] - key in spritesheet or spritemap
        
        Element <hide part=""/>
            attribute [part] - full name or "layer/*" mask

        Element <if test=""> item* </if>
            attribute [test] - evaluated as expression
            children >item* - items to process if [test] evaluates to 'true'

        Element <switch> case* default? </switch>
            optional attribute [value] - evaluated as expression; allows using case[value]
            children >case* - they are evaluated, if one passes test, its contents are processed
                and case iteration stops
            optional child >default? - processed if no cases passed the test

        Element <case test=""|value=""> item* </case>
            optional attribute [test] - evaluated as expression
            optional attribute [value] - evaluated as expression
            optional attribute [values] - evaluated as commaa-separated expressions
            children >item* - if:
                [test] is present and evaluates to 'true'
                or:
                [values] is present and any of it evaluates to same as parent
                switch[value]
                or:
                [value] is present and evaluates to same as parent switch[value]
                then items are processed

        Element <default> item* </default>

        -->
        <switch value="antennae">
            <case value="0"><!--ANTENNAE_NONE--></case>
            <case value="2"><!--ANTENNAE_BEE-->
                <show part="antennae/bee"/>
            </case>
        </switch>

        <switch value="hornType">
            <case value="0"><!--HORNS_NONE--></case>
            <case value="1"><!--HORNS_DEMON-->
                <show part="horns/2small"/>
            </case>
            <case value="2"><!--HORNS_COW_MINOTAUR-->
                <switch>
                    <case test="horns lt 3"><!--Two tiny horn-like nubs...-->
                        <show part="horns/2small"/>
                    </case>
                    <case test="horns lt 6"><!--Two moderately sized horns...-->
                        <show part="horns/2small"/>
                    </case>
                    <case test="horns lt 3"><!--Two large horns...-->
                        <show part="horns/2large"/>
                    </case>
                    <case test="horns lt 3"><!--Two very large and dangerous looking horns...-->
                        <show part="horns/2large"/>
                    </case>
                    <default><!--Two huge horns...-->
                        <show part="horns/2large"/>
                    </default>
                </switch>
            </case>
            <case value="11">
                <show part="horns/orchid"/>
                <show part="horns_bg/orchid"/>
            </case>
            <default>
                <show part="horns/2large"/>
            </default>
        </switch>

        <switch value="earType">
            <case value="0"><!--EARS_HUMAN-->
                <show part="ears_bg/human"/>
            </case>
            <case value="2"><!--EARS_DOG-->
                <show part="ears/wolf"/>
            </case>
            <case value="5"><!--EARS_CAT-->
                <show part="ears/cat"/>
            </case>
            <case value="9"><!--EARS_FOX-->
                <show part="ears/fox"/>
                <show part="ears_bg/fox"/>
            </case>
            <case value="18"><!--EARS_WOLF-->
                <show part="ears/wolf"/>
            </case>
            <case value="20"><!--EARS_YETI-->
                <show part="ears/fur"/>
            </case>
            <case value="21"><!--EARS_ORCA-->
                <show part="ears/orca"/>
            </case>
            <default>
                <show part="ears_bg/human"/>
            </default>
        </switch>

        <switch value="eyeType">
            <case value="0">
                <show part="eyes/human"/><!--EYES_HUMAN-->
            </case>
            <case value="1">
                <show part="eyes/spider"/><!--EYES_FOUR_SPIDER_EYES-->
            </case>
            <case value="2">
                <show part="eyes/sandtrap"/><!--EYES_BLACK_EYES_SAND_TRAP-->
            </case>
            <case value="3">
                <show part="eyes/cat"/><!--EYES_CAT_SLITS-->
            </case>
            <case value="6">
                <show part="eyes/manticore"/><!--EYES_MANTICORE-->
            </case>
            <case value="'never'">
                <show part="eyes/orca"/>
            </case>
            <default>
                <show part="eyes/human"/>
            </default>
        </switch>

        <switch value="hairType">
            <case value="6"><!--HAIR_GORGON-->
                <if test="hairLength > 0">
                    <show part="hair/gorgon"/>
                    <show part="hair_bg/gorgon"/>
                </if>
            </case>
            <default>
                <switch>
                    <case test="hairLength == 0"><!--bald--></case>
                    <case test="hairLength lt 6">
                        <show part="hair/0"/>
                        <show part="hair_bg/0"/>
                    </case>
                    <default>
                        <show part="hair/0"/>
                        <show part="hair_bg/0"/>
                    </default>
                </switch>
            </default>
        </switch>

        <switch value="skinType">
            <case value="'plain'">
                <show part="head/human"/>
            </case>
            <case value="'plain'">
                <show part="head/fur"/>
            </case>
            <case value="'plain'">
                <show part="head/scales"/>
            </case>
            <case value="'plain'">
                <show part="head/scales2"/>
            </case>
            <case value="'plain'">
                <show part="head/chitin"/>
            </case>
            <case value="'plain'">
                <show part="head/bee"/>
            </case>
            <case value="'plain'">
                <show part="head/orca"/>
            </case>
            <default>
                <show part="head/human"/>
            </default>
        </switch>
        <switch value="faceType">
            <case value="0"><!--FACE_HUMAN-->
                <show part="face/human"/>
            </case>
            <case values="2,6,24"><!--FACE_DOG,FACE_CAT,FACE_BWOLF-->
                <show part="face/fur"/>
            </case>
            <case value="28"><!--FACE_ORCA-->
                <show part="face/orca"/>
            </case>
            <case value="'never'">
                <show part="face/insect"/>
            </case>
            <case value="'never'">
                <show part="face/Spider"/>
            </case>
            <case value="'never'">
                <show part="face/reptile"/>
            </case>
            <case value="'never'">
                <show part="face/human_fang"/>
            </case>
            <default>
                <show part="face/human"/>
            </default>
        </switch>

        <if test="breastRows.length gt 0">
            <switch>
                <case test="breastRows[0].breastRating lte 1">
                    <show part="breasts/0"/>
                </case>
                <case test="breastRows[0].breastRating lte 10">
                    <switch>
                        <case test="hasFullCoatOfType([1])">
                            <show part="breasts/Dfur"/>
                        </case>
                        <case value="'never'">
                            <show part="breasts/Dchitin"/>
                        </case>
                        <case value="'never'">
                            <show part="breasts/Dbee"/>
                        </case>
                        <case value="'never'">
                            <show part="breasts/Dscales"/>
                        </case>
                        <case value="'never'">
                            <show part="breasts/Dscales2"/>
                        </case>
                        <default>
                            <show part="breasts/D"/>
                        </default>
                    </switch>
                </case>
                <default>
                    <show part="breasts/D"/>
                </default>
            </switch>
        </if>

        <switch value="armType">
            <case value="0"><!--ARM_TYPE_HUMAN-->
                <show part="arms/human"/>
                <show part="arms_bg/human"/>
            </case>
            <case value="11"><!--ARM_TYPE_LION-->
                <show part="neck/manticore"/>
                <show part="arms/manticore"/>
            </case>
            <case value="17"><!--ARM_TYPE_ORCA-->
                <show part="arms_bg/orca"/>
                <show part="arms/orca"/>
                <show part="arms/fins-orca"/>
                <show part="arms_bg/fins-orca"/>
            </case>
            <case value="'never'">
                <show part="arms/fur"/>
                <show part="arms_bg/fur"/>
            </case>
            <case value="'never'">
                <show part="arms/scales"/>
                <show part="arms_bg/scales"/>
            </case>
            <case value="'never'">
                <show part="arms/scales2"/>
                <show part="arms_bg/scales2"/>
            </case>
            <case value="'never'">
                <show part="arms/bee"/>
                <show part="arms_bg/bee"/>
            </case>
            <case value="'never'">
                <show part="arms/bee2"/>
                <show part="arms_bg/bee2"/>
            </case>
            <case value="'never'">
                <show part="arms/chitin"/>
                <show part="arms_bg/chitin"/>
            </case>
            <case value="'never'">
                <show part="arms/chitin2"/>
                <show part="arms_bg/chitin2"/>
            </case>
            <case value="'never'">
                <show part="arms/alraune"/>
                <show part="arms_bg/alraune"/>
            </case>
            <default>
                <show part="arms/human"/>
                <show part="arms_bg/human"/>
            </default>
        </switch>

        <switch value="lowerBody">
            <case value="0"><!--LOWER_BODY_TYPE_HUMAN-->
                <show part="legs/human"/>
            </case>
            <case values="2,9,33"><!--DOG,CAT,WOLF-->
                <show part="legs/furpaws"/>
            </case>
            <case value="3"><!--LOWER_BODY_TYPE_NAGA-->
                <show part="legs/naga"/>
            </case>
            <case value="35"><!--LOWER_BODY_TYPE_LION-->
                <switch>
                    <case test="armType==11">
                        <show part="legs/manticore_sit"/>
                    </case>
                    <default>
                        <show part="legs/furpaws"/>
                    </default>
                </switch>
            </case>
            <case value="26"><!--LOWER_BODY_TYPE_SCYLLA-->
                <show part="legs/scylla"/>
            </case>
            <case value="37"><!--LOWER_BODY_TYPE_ORCA-->
                <show part="legs/orca"/>
            </case>
            <case value="'never'">
                <show part="legs/kraken"/>
            </case>
            <case value="'never'">
                <show part="legs/chitin"/>
            </case>
            <case value="'never'">
                <show part="legs_bg/drider"/>
            </case>
            <case value="'never'">
                <show part="legs/Spider"/>
            </case>
            <case value="'never'">
                <show part="legs_bg/centaur"/>
            </case>
            <case value="'never'">
                <show part="legs/bee"/>
            </case>
            <case value="'never'">
                <show part="legs/scales"/>
            </case>
            <case value="'never'">
                <show part="legs/scales2"/>
            </case>
            <case value="'never'">
                <show part="legs_bg/alraune"/>
            </case>
            <case value="'never'">
                <show part="legs_bg/liliraune"/>
            </case>
            <default>
                <show part="legs/human"/>
            </default>
        </switch>

        <switch>
            <case test="hasFullCoatOfType([1])">
                <show part="torso/fur"/>
            </case>
            <case test="skinColor == 'black and white'">
                <show part="torso/orca"/>
            </case>
            <case value="'never'">
                <show part="torso/bee"/>
            </case>
            <case value="'never'">
                <show part="torso/chitin"/>
            </case>
            <case value="'never'">
                <show part="torso/scales"/>
            </case>
            <case value="'never'">
                <show part="torso/scales2"/>
            </case>
            <default>
                <show part="torso/human"/>
            </default>
        </switch>

        <switch value="wingType">
            <case value="0"><!--WING_TYPE_NONE--></case>
            <case value="23"><!--WING_TYPE_MANTICORE_LIKE_LARGE-->
                <show part="wings/mantibig"/>
                <show part="wings_bg/mantibig"/>
            </case>
            <case value="'never'">
                <show part="wings/bee"/>
            </case>
            <case value="'never'">
                <show part="wings/scales_left"/>
                <show part="wings/scales_right"/>
            </case>
            <default>
            </default>
        </switch>

        <switch value="tailType">
            <case value="0"><!--TAIL_TYPE_NONE--></case>
            <!--<case values="2,29">&lt;!&ndash;DOG,WOLF&ndash;&gt;
                <show part="tail-wolf"/>
            </case>-->
            <case value="8"><!--CAT-->
                <switch value="tailCount">
                    <case value="1">
                        <show part="tail/cat"/>
                    </case>
                    <case value="2">
                        <show part="tail/cat2"/>
                    </case>
                    <default>
                        <show part="tail/cat2"/>
                    </default>
                </switch>
            </case>
            <case value="13"><!--TAIL_TYPE_FOX-->
                <show part="tail/fox1"/>
                <if test="tailCount gte 2">
                    <show part="tail/fox2"/>
                </if>
                <!--<if test="tailCount gte 3">
                    <show part="tail-fox3"/>
                </if>-->
            </case>
            <case value="28"><!--TAIL_TYPE_MANTICORE_PUSSYTAIL-->
                <show part="tail/manticore"/>
            </case>
            <case value="31"><!--TAIL_TYPE_ORCA-->
                <show part="tail/orca"/>
            </case>
            <case value="'never'">
                <show part="tail/bee_abdomen"/>
            </case>
            <case value="'never'">
                <show part="tail/demon"/>
            </case>
            <case value="'never'">
                <show part="tail/reptile"/>
            </case>
            <default>
            </default>
        </switch>

    </logic>
</model>